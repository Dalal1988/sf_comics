{% extends "base.html.twig" %}

{% block body %}


<table class="table">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Country</th>
    </tr>
  </thead>
  {% for writer in writers %}
  <tbody>
    <tr>
      <th scope="row">{{ writer.id }}</th>
      <td>{{ writer.name }}</td>
      <td>{{ writer.country }}</td>
    </tr>
  </tbody>
  {% endfor %}
</table>
		

{% endblock %}